        @*<div class="col-sm-6 col-md-6 breadcrumb">
                @Html.MvcSiteMap().SiteMapPath()
            </div>*@

        <div class="main-grid">
            <div class="agile-grids">
                @RenderBody()
            </div>
        </div>
